<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="daielchom.qrtracker.Login"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="93dp"
        android:layout_height="29dp"
        android:text="@string/txBienvenido"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="381dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txIdOfficial"
        app:layout_constraintVertical_bias="0.795" />

    <ImageView
        android:layout_width="241dp"
        android:layout_height="255dp"
        android:src="@drawable/logo"
        tools:layout_editor_absoluteY="16dp"
        tools:layout_editor_absoluteX="40dp"
        android:id="@+id/imageView" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="vertical"
        app:layout_constraintGuide_end="293dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="299dp" />

    <EditText
        android:id="@+id/txIdOfficial"
        android:layout_width="248dp"
        android:layout_height="54dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/txIDOfficial"
        tools:layout_editor_absoluteY="128dp"
        tools:layout_editor_absoluteX="314dp" />

    <Button
        android:id="@+id/btIngresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btIngresar"
        tools:layout_editor_absoluteY="208dp"
        tools:layout_editor_absoluteX="392dp" />

</android.support.constraint.ConstraintLayout>
